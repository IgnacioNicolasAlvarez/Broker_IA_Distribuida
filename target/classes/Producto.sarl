class Producto extends Object {
	
	var codigo : Integer
	var precio : Double
	var stock : Integer
	
	new (v : Integer, x : Double, s : Integer)
	{
		codigo = v
		precio = x
		stock = s
	}
	
	def getCodigo : Integer
	{
		this.codigo
	}

	def getStock : Integer {
		this.stock
	}

	def setStock(x : Integer) {
		this.stock = x
	}
	
	def getPrecio : Double
	{
		this.precio
	}
}
